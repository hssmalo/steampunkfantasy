import data


darkelf = data.Race.from_toml("darkelf")

knut = data.Team("Knuts DarkElf")
knut.add_unit(darkelf.units.mechanical_assault_spider)
knut.add_unit(darkelf.units.queen_yy)
knut.add_unit(darkelf.units.assasin, name = 'Mortar Assasin')
knut.add_equipment('Mortar Assasin', darkelf.equipments.mortar1a)
knut.add_equipment('Mortar Assasin', darkelf.equipments.mechanical_imp)
knut.add_unit(darkelf.units.darkelf_infantry, name = 'crossbow men')
knut.add_equipment('crossbow men', darkelf.equipments.crossbow)
knut.add_equipment('crossbow men', darkelf.equipments.hide)
knut.add_unit(darkelf.units.scout)
